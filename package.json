{
  "name": "fbi",
  "repository": "https://github.com/DavidJFelix/fbi",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "description": "File Byte Inspector - A file type fingerprinter for typescript/javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "David J. Felix <felix.davidj@gmail.com>",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
